# application.yml.example

spring:
  application:
    name: appName
  datasource:
    url: jdbc:mysql://localhost:3306/mydb
    username: yourUsername
    password: yourPassword
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: none
    show-sql: true
    properties:
      hibernate:
        format_sql: true

kakao:
  api-key: yourApiKey

openapi:
  url: sample
  key: sample